class Solution {
    public int fib(int n) {
        //int a = 0; 
        //int b = 1;
        // if(n == 0) return a;
        // else if(n==1) return b;
        // else {
        //     for(int i = 0 ; i < n - 1 ; i++){
        //         int temp = b;
        //         b = a + b;
        //         a = temp;
        //     }
        // }
        // return b;
        
        if(n == 0 ) return 0;
        if( n == 1 ) return 1;
        return fib(n-1) + fib(n-2);
    }
}